        "symfony/form": ">=4.0 <5.2",
        "symfony/validator": ">=4.0 <5.2",
        "symfony/config": ">=4.2 <5.2",
        "symfony/twig-bridge": ">=4.0 <5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Georget",
            "email": "laurent.georget@meteo-concept.fr"
